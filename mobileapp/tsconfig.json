{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "api": ["api"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "locales/*": ["locales/*"],
      "utils/*": ["utils/*"],
    }
  }
}
